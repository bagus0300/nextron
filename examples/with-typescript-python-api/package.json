{
  "private": true,
  "name": "my-nextron-python-app",
  "description": "My application description",
  "version": "1.0.0",
  "author": "Don Alvarez",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.nextronpythonapi",
    "productName": "My Nextron Python App",
    "copyright": "Copyright Â© 2019 Don Alvarez",
    "directories": {
      "output": "dist",
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "dev": "node nextron/nextron.js",
    "build-python": "shx rm -rf pythondist && pyinstaller python/api.py --distpath pythondist --workpath build-py-temp --onefile && shx rm -rf build-py-temp && shx rm api.spec",
    "build-nextron": "node nextron/nextron.js build",
    "build": "npm run build-python && npm run build-nextron"
  },
  "keywords": [
    "Nextron",
    "Electron",
    "NextJS",
    "Python",
    "Flask",
    "demo"
  ],
  "dependencies": {
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.14",
    "cross-spawn": "^6.0.5",
    "electron-store": "^3.2.0",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "isomorphic-fetch": "^2.2.1",
    "next": "8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "superagent": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/next": "^8.0.3",
    "@types/node": "^11.13.4",
    "@types/react": "^16.8.13",
    "@zeit/next-typescript": "1.1.1",
    "arg": "^4.1.0",
    "chalk": "^2.4.2",
    "electron": "5.0.0",
    "electron-builder": "20.39.0",
    "electron-packager": "^9.0.1",
    "electron-rebuild": "^1.8.4",
    "fast-glob": "^2.2.6",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^7.0.1",
    "node-npx": "^1.3.1",
    "npm-run-all": "^4.1.3",
    "ora": "^3.4.0",
    "shx": "^0.3.2",
    "typescript": "^3.4.5",
    "webpack": "4.29.0",
    "webpack-merge": "^4.2.1"
  }
}
