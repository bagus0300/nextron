{
  "name": "nextronpy",
  "description": "Nextron, Electron, NextJS, and Python sample",
  "version": "1.0.0",
  "author": "Don Alvarez",
  "main": "app/background.js",
  "build": {
    "appId": "com.example.nextronpy",
    "productName": "My App",
    "copyright": "Copyright Â© 2018 ${author}",
    "directories": {
      "output": "dist",
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "dev-python": "shx rm -rf pythondist && python python/api.py",
    "dev-node": "node build/nextron.js",
    "dev": "run-p dev-python dev-node",
    "build-python": "shx rm -rf pythondist && pyinstaller python/api.py --distpath pythondist --workpath build-py-temp && shx rm -rf build-py-temp && shx rm api.spec",
    "build-nextron": "node build/nextron.js build",
    "package": "electron-packager . --overwrite --ignore=\"python$\" --ignore=\"README.md\" --ignore=\"package.json\"",
    "build": "npm run build-python && npm run build-nextron && npm run package"
  },
  "keywords": [
    "Nextron",
    "Electron",
    "NextJS",
    "Python",
    "Flask",
    "demo"
  ],
  "dependencies": {
    "@types/isomorphic-fetch": "0.0.34",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-cache-inmemory": "^1.3.9",
    "apollo-client": "^2.4.5",
    "apollo-link-http": "^1.5.5",
    "fs-jetpack": "^2.2.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "isomorphic-fetch": "^2.2.1",
    "next": "7.0.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@types/next": "^7.*",
    "@types/react": "^16.*",
    "arg": "^2.0.0",
    "chalk": "^2.4.1",
    "cross-spawn": "^6.0.5",
    "electron": "3.0.8",
    "electron-builder": "20.34.0",
    "electron-packager": "^9.0.1",
    "electron-rebuild": "^1.8.2",
    "fast-glob": "^2.2.4",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^7.0.1",
    "node-npx": "^1.3.1",
    "npm-run-all": "^4.1.3",
    "ora": "^3.0.0",
    "shx": "^0.3.2",
    "ts-loader": "^5.3.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.4.0",
    "typescript": "^3.1.6",
    "webpack": "4.20.2",
    "webpack-merge": "^4.1.4"
  }
}
